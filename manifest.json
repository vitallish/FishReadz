{
  "name": "Fish Readz",
  "version": "0.3.2",
  "manifest_version":2,
  "description": "Readz your turn RSS feeds from Warfish.net",
  "permissions":[
	"http://warfish.net/*","alarms","tabs","storage"
  ],
  "homepage_url": "http://www.warfish.net",
  "background": {
	"scripts":["js/jquery-2.1.1.min.js","js/polling.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
	"default_popup": "html/popup.html"
  },
  "options_page": "html/options.html",
  "icons": { "16": "img/icon16.png",
           "48": "img/icon48.png",
          "128": "img/icon128.png" }
}